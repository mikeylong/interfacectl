    "test": "node --test test/**/*.test.mjs"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "typescript": "^5.6.3"
  }
}

